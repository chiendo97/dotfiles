# Example API keys file - copy and encrypt with:
# age -r "$(cat ~/.ssh/id_ed25519_agenix.pub)" -o api-keys.age api-keys.txt

# AI/LLM
export ANTHROPIC_API_KEY="sk-ant-your-key-here"
export OPENROUTER_API_KEY="sk-or-v1-your-key-here"
export OPENAI_API_KEY=$OPENROUTER_API_KEY
export OPENAI_BASE_URL="https://openrouter.ai/api/v1"
export DEEPSEEK_API_KEY="your-key-here"
export GEMINI_API_KEY="your-key-here"
export MISTRAL_API_KEY="your-key-here"
export PERPLEXITY_API_KEY="your-key-here"

# AIChat
export AICHAT_CONFIG_DIR=$HOME/.config/aichat
export AICHAT_FUNCTIONS_DIR=$HOME/.config/aichat/llm-functions

# Postman
export POSTMAN_API_KEY="your-key-here"

# Services
export TELEGRAM_BOT_TOKEN="your-token-here"
export TODOIST_API_KEY="your-key-here"
export GITLAB_TOKEN="your-token-here"
export GITHUB_TOKEN="your-token-here"

# Notion
export NOTION_TOKEN="your-notion-token-here"
export NOTION_DATABASE_ID="your-database-id-here"

# Discord
export DISCORD_TOKEN="your-discord-token-here"

# GitHub App
export GITHUB_APP_ID="your-app-id-here"
export GITHUB_APP_INSTALLATION_ID="your-installation-id-here"
export GITHUB_APP_PRIVATE_KEY='your-private-key-here'

# Android (macOS)
export ANDROID_HOME=$HOME/Library/Android/sdk
